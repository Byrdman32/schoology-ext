{
  "manifest_version": 2,
  "name": "Schoology Completion Extension",
  "description": "Puts check marks next to completed assignments after you click on them.",
  "version": "1.3.10",
  "author": "Jacob Sadowyj",
  "content_scripts": [
    {
      "matches": [
        "https://schoology.monroeisd.us/",
        "https://schoology.monroeisd.us/home",
        "https://app.schoology.com/",
        "https://app.schoology.com/home",
        "https://fuhsd.schoology.com/",
        "https://fuhsd.schoology.com/home",
        "https://schoology.westportps.org/",
        "https://schoology.westportps.org/home",
        "https://*.schoology.com/",
        "https://*.schoology.com/home"
      ],
      "js": ["src/jquery-3.5.0.min.js", "src/shared.js", "src/async-home.js"]
    },
    {
      "matches": [
        "https://schoology.monroeisd.us/assignment/*",
        "https://app.schoology.com/assignment/*",
        "https://fuhsd.schoology.com/assignment/*",
        "https://schoology.westportps.org/assignment/*",
        "https://*.schoology.com/assignment/*"
      ],
      "js": [
        "src/jquery-3.5.0.min.js",
        "src/shared.js",
        "src/async-assignment.js"
      ]
    },
    {
      "matches": [
        "https://schoology.westportps.org/calendar/*"
      ],
      "js": [
        "src/jquery-3.5.0.min.js",
        "src/shared.js",
        "src/async-calendar.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "public/popup.html",
    "default-icon": "public/schoology-icon.png"
  },
  "permissions": ["storage"],
  "background": {
    "scripts": ["src/jquery-3.5.0.min.js"]
  },
  "icons": {
    "16": "public/schoology-icon.png",
    "48": "public/schoology-icon.png",
    "128": "public/schoology-icon.png"
  }
}
